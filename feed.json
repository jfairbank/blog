---
layout: null
sitemap: false
---
{% assign first = true %}
processFeed({
  "posts": [
    {% for post in site.posts %}
      {% if post.title != null and post.title != empty %}
        {% unless first %},{% endunless %}{
          "title": {{post.title | jsonify}},
          "link": "{{ site.url }}{{ post.url }}",
          "date": "{{ post.date }}",
          "excerpt": "{{ post.snippet }}"
        }
        {% assign first = false %}
      {% endif %}
    {% endfor %}
  ]
})
